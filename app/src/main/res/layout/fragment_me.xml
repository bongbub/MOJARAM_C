<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="com.example.mojaram.meFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

            <androidx.fragment.app.FragmentContainerView
                android:id="@+id/fgcontainer_me_to_myRiviews"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:name="androidx.navigation.fragment.NavHostFragment"
                app:navGraph="@navigation/me_to_myreviews_nav"
                app:defaultNavHost="true"/>

        <com.google.android.material.navigation.NavigationView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="start|top"/>

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginEnd="224dp"
                    android:layout_marginBottom="18dp"
                    android:paddingLeft="40dp"
                    android:paddingTop="50dp"
                    android:text="마이페이지"
                    android:textAlignment="center"
                    android:textSize="32sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toTopOf="@+id/button"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:orientation="horizontal"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/button"/>

                <Button
                    android:id="@+id/button"
                    style="@style/Widget.Material3.Button"
                    android:layout_width="347dp"
                    android:layout_height="114dp"
                    android:layout_marginStart="32dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="32dp"
                    android:layout_marginBottom="5dp"
                    android:background="@drawable/button_background"
                    android:elevation="18dp"
                    android:gravity="center"
                    android:insetTop="0dp"
                    android:text="김미소"
                    android:textColor="#000000"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    app:backgroundTint="#FFFFFF"
                    app:layout_constraintBottom_toTopOf="@+id/divider"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView3" />


                <TextView
                    android:id="@+id/mimisosoTextView"
                    android:layout_width="64dp"
                    android:layout_height="19dp"
                    android:layout_marginStart="175dp"
                    android:layout_marginTop="180dp"
                    android:text="mimisoso"
                    android:textColor="#B2434242"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/arrow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="328dp"
                    android:layout_marginTop="132dp"
                    android:text=">"
                    android:textColor="#B2434242"
                    android:textSize="50dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <View
                    android:id="@+id/divider"
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_marginStart="32dp"
                    android:layout_marginTop="30dp"
                    android:layout_marginEnd="32dp"
                    android:layout_marginBottom="22dp"
                    android:background="#7F7F7F"
                    app:layout_constraintBottom_toTopOf="@+id/btn_my1"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/button" />


                <Button
                    android:id="@+id/btn_my1"
                    style="@style/Widget.Material3.Button"
                    android:layout_width="200dp"
                    android:layout_height="40dp"
                    android:minWidth="48dp"
                    android:minHeight="48dp"
                    android:layout_marginStart="60dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="105dp"
                    android:layout_marginBottom="10dp"
                    android:background="@drawable/button_background"
                    android:gravity="center"
                    android:insetTop="0dp"
                    android:text="내가 쓴 리뷰"
                    android:textAlignment="textStart"
                    android:textColor="#000000"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    app:backgroundTint="#FFFFFF"
                    app:layout_constraintBottom_toTopOf="@+id/divider1"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.03"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/divider" />

                <View
                    android:id="@+id/divider1"
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_marginStart="70dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="70dp"
                    android:layout_marginBottom="5dp"
                    android:background="#D3D3D3"
                    app:layout_constraintBottom_toTopOf="@+id/btn_my2"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/btn_my1" />

                <Button
                    android:id="@+id/btn_my2"
                    style="@style/Widget.Material3.Button"
                    android:layout_width="200dp"
                    android:layout_height="40dp"
                    android:minWidth="48dp"
                    android:minHeight="48dp"
                    android:layout_marginStart="60dp"
                    android:layout_marginTop="9dp"
                    android:layout_marginEnd="105dp"
                    android:layout_marginBottom="9dp"
                    android:background="@drawable/button_background"
                    android:gravity="center"
                    android:insetTop="0dp"
                    android:text="예약 내역 확인"
                    android:textAlignment="textStart"
                    android:textColor="#000000"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    app:backgroundTint="#FFFFFF"
                    app:layout_constraintBottom_toTopOf="@+id/divider3"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.03"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/divider1" />

                <View
                    android:id="@+id/divider3"
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_marginStart="70dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="70dp"
                    android:layout_marginBottom="5dp"
                    android:background="#D3D3D3"
                    app:layout_constraintBottom_toTopOf="@+id/btn_my3"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/btn_my2" />

                <Button
                    android:id="@+id/btn_my3"
                    style="@style/Widget.Material3.Button"
                    android:layout_width="200dp"
                    android:layout_height="40dp"
                    android:minWidth="48dp"
                    android:minHeight="48dp"
                    android:layout_marginStart="60dp"
                    android:layout_marginTop="12dp"
                    android:layout_marginEnd="105dp"
                    android:layout_marginBottom="8dp"
                    android:background="@drawable/button_background"
                    android:gravity="center"
                    android:insetTop="0dp"
                    android:text="상담 내역 확인"
                    android:textAlignment="textStart"
                    android:textColor="#000000"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    app:backgroundTint="#FFFFFF"
                    app:layout_constraintBottom_toTopOf="@+id/divider4"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.03"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/divider3" />

                <View
                    android:id="@+id/divider4"
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_marginStart="70dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="70dp"
                    android:layout_marginBottom="5dp"
                    android:background="#D3D3D3"
                    app:layout_constraintBottom_toTopOf="@+id/btn_my4"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/btn_my3" />

                <Button
                    android:id="@+id/btn_my4"
                    style="@style/Widget.Material3.Button"
                    android:layout_width="200dp"
                    android:layout_height="40dp"
                    android:minWidth="48dp"
                    android:minHeight="48dp"
                    android:layout_marginStart="60dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="105dp"
                    android:layout_marginBottom="8dp"
                    android:background="@drawable/button_background"
                    android:gravity="center"
                    android:insetTop="0dp"
                    android:text="찜 목록"
                    android:textAlignment="textStart"
                    android:textColor="#000000"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    app:backgroundTint="#FFFFFF"
                    app:layout_constraintBottom_toTopOf="@+id/divider5"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/divider4" />

                <View
                    android:id="@+id/divider5"
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_marginStart="70dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="70dp"
                    android:layout_marginBottom="5dp"
                    android:background="#D3D3D3"
                    app:layout_constraintBottom_toTopOf="@+id/btn_my5"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/btn_my4" />

                <Button
                    android:id="@+id/btn_my5"
                    style="@style/Widget.Material3.Button"
                    android:layout_width="200dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="60dp"
                    android:layout_marginTop="12dp"
                    android:layout_marginEnd="105dp"
                    android:layout_marginBottom="146dp"
                    android:background="@drawable/button_background"
                    android:gravity="center"
                    android:insetTop="0dp"
                    android:text="설정"
                    android:textAlignment="textStart"
                    android:textColor="#000000"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    app:backgroundTint="#FFFFFF"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.03"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/divider5" />

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginStart="35dp"
                    android:src="@drawable/review"
                    app:layout_constraintBottom_toBottomOf="@+id/btn_my1"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/btn_my1" />


                <ImageView
                    android:id="@+id/imageView4"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginStart="35dp"
                    android:src="@drawable/reservation"
                    app:layout_constraintBottom_toBottomOf="@+id/btn_my2"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/btn_my2" />

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginStart="35dp"
                    android:src="@drawable/consultation"
                    app:layout_constraintBottom_toBottomOf="@+id/btn_my3"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/btn_my3" />

                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginStart="35dp"
                    android:src="@drawable/bookmark"
                    app:layout_constraintBottom_toBottomOf="@+id/btn_my4"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/btn_my4" />

                <ImageView
                    android:id="@+id/imageView7"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginStart="35dp"
                    android:src="@drawable/settings"
                    app:layout_constraintBottom_toBottomOf="@+id/btn_my5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/btn_my5" />

            </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>