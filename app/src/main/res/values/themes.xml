<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.Mojaram" parent="Theme.Material3.DayNight.NoActionBar">
        <item name = "android:windowLightStatusBar">true</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorPrimary">@color/maincolor</item>




    </style>

    <style name="Theme.Mojaram" parent="Base.Theme.Mojaram" />
    <!-- Base application theme. -->
    <style name="Theme.FirstWeek" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/white</item>
        <!-- Customize your theme here. -->
        <item name="windowNoTitle">true</item>
        <item name="android:windowLightStatusBar">true</item>
    </style>

    <style name="tab_text" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">17sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="collapse" parent="android:style/TextAppearance">
        <item name="android:drawableStart">@drawable/ic_baseline_search_24</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="circleImageView" parent="">
        <item name="cornerSize">50%</item>
    </style>

    <style name="radius_10" parent="">
        <item name="cornerSize">10%</item>
    </style>

    <style name="radius_5" parent="">
        <item name="cornerSize">5%</item>
    </style>
</resources>